{
  "name": "@redstone/redstone-form",
  "version": "0.0.1",
  "private": false,
  "author": "Ozkey",
  "devDependencies": {
    "antd": "^3.4.1",
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^21.2.0",
    "jest-cli": "^22.4.2"
  },
  "dependencies": {
    "dot-prop-immutable": "^1.4.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "lodash.get": "^4.4.2",
    "lodash.has": "^4.5.2",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "start": "cross-env PORT=8787 react-scripts start",
    "build": "react-scripts build",
    "testWatch": "react-scripts test --env=jsdom",
    "test": "cross-env NODE_ENV=unit-test jest --no-cache",
    "comment:============================= BUILD TOOLS  =============================": "",
    "cleanPublish": "rm -rf publish",
    "cleanPublisWindows": "rmdir publish /s /q",
    "cleanBuildWindows": "rmdir build /s /q",
    "build:es2015": "babel --presets es2015,stage-0 --plugins transform-react-jsx -d publish src/redstone-form"
  },
  "jest": {
    "verbose": false,
    "testRegex": "((\\.|/)(spec))\\.jsx?$",
    "collectCoverage": true,
    "coverageDirectory": "./build/coverage",
    "collectCoverageFrom": [
      "**/src/redstone-form/**",
      "!<rootDir>/node_modules/"
    ],
    "coverageReporters": [
      "text"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/publish/",
      "<rootDir>/build/"
    ],
    "setupFiles": [
      "<rootDir>src/redstone-form/tests/setup/setupTests.js"
    ]
  }
}
